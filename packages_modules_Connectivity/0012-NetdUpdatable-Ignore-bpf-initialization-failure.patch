Subject: [PATCH 12/13] NetdUpdatable: Ignore bpf initialization failure

Change-Id: Ib62ec44592cd11a628d7e95eef89edda486dbed0
---
 netd/NetdUpdatable.cpp | 3 +--
 1 file changed, 1 insertion(+), 2 deletions(-)

diff --git a/netd/NetdUpdatable.cpp b/netd/NetdUpdatable.cpp
index 3b15916398..f4af63c522 100644
--- a/netd/NetdUpdatable.cpp
+++ b/netd/NetdUpdatable.cpp
@@ -31,8 +31,7 @@ int libnetd_updatable_init(const char* cg2_path) {
 
     android::netdutils::Status ret = sBpfHandler.init(cg2_path);
     if (!android::netdutils::isOk(ret)) {
-        LOG(ERROR) << __func__ << ": Failed: (" << ret.code() << ") " << ret.msg();
-        abort();
+        LOG(ERROR) << __func__ << ": Failed. (" << ret.code() << ") " << ret.msg();
     }
     return 0;
 }
